[user]
	name = Daniel Moberg
	email = x
[alias]
	co = checkout
	br = branch
	st = status
	comm = commit -av
	amend = commit --amend
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[push]
	default = simple
[core]
	excludesfile = /Users/danielmoberg/.gitignore_global
	editor = /usr/bin/vim
	precomposeunicode = true
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
[credential]
	helper = osxkeychain
